extends ../../layouts/default

include ../../mixins/organiser

block intro
	.page-intro: .container
		h1.page-title Trade

block content
	.page-content
		.section.highlight: .container
			//- h2 Organisers
			//- p.lead Each month the SydJS team works to bring you great presenters in a great venue.
			.row.about-organisers
		//- CODE
		.section: .container-sm
			h2 Code of Conduct
			// Editor will go here
			pre#editor_container.
				function foo(items) {
					var i;
					for (i = 0; i < items.length; i++) {
						alert("Ace Rocks " + items[i]);
					}
				}
			script(src='/js/requirejs/require.js')
			//script(src='/js/ace/demo/kitchen-sink/require.js')
			script.
				require.config({paths: {ace: "/js/ace/build/src"}});
				define('testace', ['ace/ace'],
				function(ace, langtools) {
					console.log("This is the testace module");
					var editor = ace.edit("editor_container");
					editor.setTheme('ace/theme/eclipse');
					editor.session.setMode('ace/mode/javascript');
					require(["/js/ace/lib/ace/requirejs/text!src/ace"], function(e){
						editor.setValue(e);
					})
				});
				require(['testace']);

block js
	// twitter
	script.
		!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");